/**app.wxss**/
@import "/pages/wxParse/wxParse.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.new-1 {
  display: flex;
  justify-content: space-between;
  justify-items: center;
}

.box1 {
  display: flex;
  justify-items: center;
}

.new-1 .box1 text {
  padding: 0 16rpx;
  color: rgba(51, 51, 51, 1);
}

.new-1 .box1 image {
  width: 14rpx;
  height: 38rpx;
}
movable-area{
  position: absolute;
  width: 100%;
  height: auto;
  overflow: hidden;
  min-height: 100vh;

}
movable-view{
     display: inline-block;
width: 160rpx;
height: 160rpx; 
position: absolute;
bottom: 30rpx;
right: 30rpx;
top: auto;
left: auto;
z-index: 9999;
}