/* pages/footmark/rongyuDetails/rongyuDetails.wxss */

.top-box {
  height: 322rpx;
  position: relative;
  width: 100;
  padding-bottom: 44rpx;
}

.top-box .top-banner {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}

.top-box .rongyu-logo {
  position: absolute;
  width: 284rpx;
  height: 265rpx;
  left: 50%;
  top: 50%;
  margin-left: -144rpx;
  margin-top: -132rpx;
}

.top-box .top-title {
  position: absolute;
  display: flex;
  align-items: center;
  right: 30rpx;
  top: 30rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 36rpx;
}

.top-box .top-log {
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}

.title-01 {
  text-align: center;
  font-size: 32rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.title-01-01 {
  text-align: center;
  background: rgba(0, 100, 40, 1);
  color: #fff100;
  font-size: 48rpx;
}

.title-01-box {
  display: block;
  width: 100%;
}

.title-02 {
  width: 566rpx;
  text-align: center;
  padding: 29rpx 0 19rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(77, 77, 77, 1);
  line-height: 40rpx;
  margin: 0 auto;
}
