.order-title{
  font-size:36rpx;
  font-weight:bold;
  color:rgba(255,241,0,1);
  padding: 25rpx 0 81rpx 31rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size:100% 100%; 
}
.game-msg{
  display: flex;
  padding-top: 30rpx;
}
.game-msg>view>view:nth-child(2){
  font-size:24rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  margin-top: 19rpx;
}
.game-msg>view:first-child{
  width: 316rpx;
}
.game-msg>view:last-child{
  flex:1;
}
.game-msg-space{
  display: flex;
  align-items: center;
}
.game-msg-space>image{
  width: 24rpx;
  height: 24rpx;
  display: block;
}
.game-msg-space>text{
  margin-left: 10rpx;
  font-size:24rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.msg-title{
  display: flex;
}
.msg-title>text{
  font-size:40rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.msg-title>view{
  font-size:22rpx;
  font-weight:400;
  color:rgba(0,100,40,1);
  display: flex;
  align-items: center;
  padding: 0 19rpx;
  background:rgba(228,245,235,1);
  border-radius:4rpx;
  margin-left: 20rpx;
}
.order-msg{
  padding: 40rpx 10rpx 0 31rpx;
  background: #fff;
  border-radius:30rpx 30rpx 0px 0px;
  margin-top: -40rpx;
}
.game-content{
  font-size:24rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  margin-top: 19rpx;
}
.martop{
  margin-top: 29rpx;
}
.order-details{
  margin-top: 29rpx;
  padding-right: 20rpx;
}
.order-details-title{
  font-size:32rpx;
  font-weight:bold;
  color:rgba(51,51,51,1);
  padding-left: 29rpx;
  position: relative;
}
.order-details-title::before{
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  content: '';
  width: 9rpx;
  height: 30rpx;
  background: #007F33;
  border-right:5rpx solid #FFF100; 
}
.order-details-item{
  margin-top: 14rpx;
}
.order-details-item>view{
  font-size:28rpx;
  font-weight:400;
  color:rgba(77,77,77,1);
  line-height:28rpx;
  padding:15rpx 0 14rpx; 
}
.order-details-item>view>text{
  font-weight: bold;
}